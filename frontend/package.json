{
  "name": "biisitin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@blueprintjs/core": "^5.2.0",
    "@blueprintjs/datetime": "^5.0.8",
    "@blueprintjs/icons": "^5.1.5",
    "@blueprintjs/select": "^5.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@feathersjs/client": "^5.0.5",
    "@feathersjs/socketio-client": "^5.0.5",
    "@react-aria/focus": "^3.2.3",
    "array-move": "4.0.0",
    "classnames": "^2.2.6",
    "counterpart": "^0.18.6",
    "date-fns": "^2.30.0",
    "debug": "4.3.4",
    "events": "^3.3.0",
    "fast-deep-equal": "^3.1.1",
    "graphql": "15.8.0",
    "http-proxy-middleware": "^2.0.6",
    "key-event-to-string": "^1.2.0",
    "markdown-to-jsx": "^6.11.4",
    "node-diff3": "3.1.2",
    "partial.lenses": "^14.17.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroller": "^1.2.6",
    "react-markdown-editor-lite": "1.3.3",
    "react-qr-code": "^2.0.11",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-touch-events": "^3.0.0",
    "rfc6902": "5.0.1",
    "socket.io-client": "^4.6.1",
    "util": "^0.12.4",
    "yup": "^0.28.1"
  },
  "scripts": {
    "lint": "eslint --fix src",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prestart": "npm run codegen",
    "start": "react-scripts start",
    "prebuild": "npm run codegen",
    "build": "REACT_APP_BUILD_TIME=$(date -I) REACT_APP_COMMIT_REV=$(git show-ref --hash=6 HEAD) react-scripts build",
    "pretest": "npm run codegen",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "simple-import-sort",
      "import"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": 1,
      "@typescript-eslint/no-non-null-assertion": 1,
      "@typescript-eslint/ban-ts-comment": "error",
      "simple-import-sort/imports": [
        "error",
        {
          "groups": [
            [
              "^node:"
            ],
            [
              "react",
              "^@?\\w"
            ],
            [
              "^(backend|services)(/.*|$)"
            ],
            [
              "^(libraries)(/.*|$)",
              "^(components|pages|utils)(/.*|$)"
            ],
            [
              "^((./)?types)(/.*|$)"
            ],
            [
              "^"
            ],
            [
              "^\\."
            ],
            [
              "^\\u0000"
            ]
          ]
        }
      ],
      "simple-import-sort/exports": "error",
      "import/first": "error",
      "import/newline-after-import": "error",
      "import/no-duplicates": "error",
      "no-trailing-spaces": [
        "error"
      ],
      "comma-dangle": [
        "error",
        "only-multiline"
      ],
      "comma-spacing": 1,
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/client-preset": "1.1.0",
    "@types/jest": "29.0.0",
    "@types/node": "^20.3.0",
    "@types/react": "^17.0.61",
    "@types/react-dom": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@typescript-eslint/utils": "^5.51.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "sass": "^1.53.0",
    "source-map-explorer": "2.5.3",
    "typescript": "4.9.4"
  }
}
