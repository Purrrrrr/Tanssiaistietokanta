{
  "name": "biisitin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@apollo/client": "^3.12.7",
    "@blueprintjs/core": "^5.10.3",
    "@blueprintjs/datetime2": "^2.3.5",
    "@blueprintjs/icons": "^5.9.0",
    "@blueprintjs/select": "^5.1.5",
    "@dnd-kit/sortable": "^10.0.0",
    "@react-aria/focus": "^3.19.1",
    "@tailwindcss/vite": "^4.1.6",
    "@wojtekmaj/react-daterange-picker": "^6.0.0",
    "array-move": "4.0.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.30.0",
    "debug": "4.3.4",
    "downshift": "^9.0.9",
    "events": "^3.3.0",
    "fast-deep-equal": "^3.1.1",
    "graphql": "15.8.0",
    "http-proxy-middleware": "^2.0.6",
    "markdown-to-jsx": "^7.7.0",
    "node-diff3": "3.1.2",
    "partial.lenses": "^14.17.0",
    "react": "^18.3.1",
    "react-compiler-runtime": "^19.0.0-beta-27714ef-20250124",
    "react-date-picker": "^11.0.0",
    "react-dom": "^18.3.1",
    "react-infinite-scroller": "^1.2.6",
    "react-markdown-editor-lite": "^1.3.4",
    "react-qr-code": "^2.0.14",
    "react-router-dom": "^6.23.1",
    "react-swipeable": "^7.0.2",
    "rfc6902": "5.0.1",
    "socket.io-client": "^4.6.1",
    "tailwindcss": "^4.1.6",
    "talkr": "^3.4.0",
    "util": "^0.12.4",
    "yup": "^1.2.0"
  },
  "scripts": {
    "lint": "eslint --fix src",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prestart": "curl localhost:8082 --retry 6 --retry-connrefused -I -s > /dev/null && npm run codegen",
    "start": "vite",
    "prebuild": "npm run codegen",
    "build": "REACT_APP_BUILD_TIME=$(date -I) REACT_APP_COMMIT_REV=$(git show-ref --hash=6 HEAD) vite build",
    "pretest": "npm run codegen",
    "test": "jest --watchAll",
    "codegen": "graphql-codegen --config codegen.ts",
    "dev": "NODE_OPTIONS=--max-old-space-size=8192 vite"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react-hooks/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "browser": true,
      "es6": true
    },
    "plugins": [
      "react",
      "simple-import-sort",
      "import"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": 1,
      "@typescript-eslint/no-non-null-assertion": 1,
      "@typescript-eslint/ban-ts-comment": "error",
      "simple-import-sort/imports": [
        "error",
        {
          "groups": [
            [
              "^node:"
            ],
            [
              "react",
              "^@?\\w"
            ],
            [
              "^types(/.*)?\\u0000?$",
              "^\\./types(/.*)?\\u0000?$",
              "^\\.\\./types(/.*)?\\u0000?$",
              "^.*/types(/.*)?\\u0000?$"
            ],
            [
              "^(backend|services)(/.*|$)"
            ],
            [
              "^(libraries)(/.*|$)",
              "^(components|pages|utils|i18n)(/.*|$)"
            ],
            [
              "^"
            ],
            [
              "^\\."
            ],
            [
              "^\\u0000"
            ]
          ]
        }
      ],
      "simple-import-sort/exports": "error",
      "import/first": "error",
      "import/newline-after-import": "error",
      "import/no-duplicates": "error",
      "no-trailing-spaces": [
        "error"
      ],
      "comma-dangle": [
        "error",
        "only-multiline"
      ],
      "comma-spacing": 1,
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ],
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "varsIgnorePattern": "^_",
          "argsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_",
          "destructuredArrayIgnorePattern": "^_"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-react-jsx-development": "^7.25.9",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "7.21",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.6.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.11.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@typescript-eslint/utils": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-jest": "^29.7.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-27714ef-20250124",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-compiler": "^19.0.0-beta-27714ef-20250124",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "jest": "^29.7.0",
    "source-map-explorer": "2.5.3",
    "typescript": "5.7",
    "vite": "^6.0.11",
    "vite-bundle-analyzer": "^0.21.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
