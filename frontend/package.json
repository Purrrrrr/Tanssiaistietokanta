{
  "name": "biisitin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@blueprintjs/core": "4.11.3",
    "@blueprintjs/icons": "4.7.0",
    "@blueprintjs/select": "4.7.0",
    "@feathersjs/client": "^4.5.15",
    "@reach/router": "^1.3.4",
    "@react-aria/focus": "^3.2.3",
    "@types/cypress-axe": "^0.8.0",
    "array-move": "4.0.0",
    "classnames": "^2.2.6",
    "counterpart": "^0.18.6",
    "debug": "4.3.4",
    "events": "^3.3.0",
    "fast-deep-equal": "^3.1.1",
    "graphql": "15.8.0",
    "history": "^5.0.0",
    "http-proxy-middleware": "^2.0.6",
    "key-event-to-string": "^1.2.0",
    "markdown-to-jsx": "^6.11.4",
    "node-diff3": "3.1.2",
    "partial.lenses": "^14.17.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-infinite-scroller": "^1.2.4",
    "react-markdown-editor-lite": "1.3.3",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "5.0.1",
    "react-sortable-hoc": "^1.10.1",
    "react-touch-events": "^3.0.0",
    "socket.io-client": "^2.5.0",
    "util": "^0.12.4",
    "yup": "^0.28.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prestart": "npm run codegen",
    "start": "react-scripts start",
    "prebuild": "npm run codegen",
    "build": "REACT_APP_BUILD_TIME=$(date -I) REACT_APP_COMMIT_REV=$(git show-ref --hash=6 HEAD) react-scripts build",
    "pretest": "npm run codegen",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": ["simple-import-sort", "import"],
    "rules": {
      "@typescript-eslint/no-explicit-any": 1,
      "@typescript-eslint/no-non-null-assertion": 1,
      "@typescript-eslint/ban-ts-comment": "error",
      "simple-import-sort/imports": ["error", {
        "groups": [
          ["^node:"],
          ["react", "^@?\\w"],
          ["^(backend|services)(/.*|$)"],
          ["^(libraries)(/.*|$)", "^(components|pages|utils)(/.*|$)"],
          ["^((./)?types)(/.*|$)"],
          ["^"],
          ["^\\."],
          ["^\\u0000"]
        ]
      }],
      "simple-import-sort/exports": "error",
      "import/first": "error",
      "import/newline-after-import": "error",
      "import/no-duplicates": "error",
      "no-trailing-spaces": [
        "error"
      ],
      "comma-dangle": [
        "error",
        "only-multiline"
      ],
      "comma-spacing": 1,
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/client-preset": "1.1.0",
    "@types/jest": "29.0.0",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.47",
    "@types/react-dom": "^16.9.8",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "sass": "^1.53.0",
    "source-map-explorer": "2.5.3",
    "typescript": "4.8.3"
  }
}
